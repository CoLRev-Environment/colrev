{
  "project": {
    "id_pattern": "THREE_AUTHORS_YEAR",
    "review_type": "realtime",
    "share_stat_req": "processed",
    "delay_automated_processing": false,
    "curation_url": null,
    "curated_masterdata": false,
    "curated_fields": []
  },
  "search": {
    "sources": [
      {
        "filename": "pdfs.bib",
        "search_type": "FEED",
        "source_name": "PDFS",
        "source_identifier": "{{file}}",
        "search_parameters": "SCOPE path='pdfs'",
        "comment": ""
      }
    ]
  },
  "load": {},
  "prep": {
    "fields_to_keep": [],
    "prep_rounds": [
      {
        "name": "high_confidence",
        "similarity": 0.95,
        "scripts": [
          "load_fixes",
          "remove_urls_with_500_errors",
          "remove_broken_IDs",
          "global_ids_consistency_check",
          "prep_curated",
          "format",
          "resolve_crossrefs",
          "get_doi_from_urls",
          "get_masterdata_from_doi",
          "get_masterdata_from_crossref",
          "get_masterdata_from_dblp",
          "get_masterdata_from_open_library",
          "get_year_from_vol_iss_jour_crossref",
          "get_record_from_local_index",
          "remove_nicknames",
          "format_minor",
          "drop_fields"
        ]
      }
    ]
  },
  "dedupe": {
    "merge_threshold": 0.8,
    "partition_threshold": 0.5,
    "same_source_merges": "prevent"
  },
  "prescreen": {
    "scope": [
        {
            "LanguageScope": [
                "en"
            ]
        }
    ],
    "scripts": [
        {
            "endpoint": "scope_prescreen"
        },
        {
            "endpoint": "colrev_cli_prescreen"
        }
    ]
  },
  "pdf_get": {
    "pdf_path_type": "symlink",
    "scripts": [
      {
        "endpoint": "unpaywall"
      },
      {
        "endpoint": "local_index"
      },
      {
        "endpoint": "website_screenshot"
      }
    ]
  },
  "pdf_prep": {
    "scripts": [
      {
        "endpoint": "pdf_check_ocr"
      },
      {
        "endpoint": "remove_coverpage"
      },
      {
        "endpoint": "remove_last_page"
      },
      {
        "endpoint": "validate_pdf_metadata"
      },
      {
        "endpoint": "validate_completeness"
      }
    ]
  },
  "screen": {
    "criteria": [],
    "scripts": [
        {
            "endpoint": "colrev_cli_screen"
        }
    ]
  },
  "data": {
    "data_format": [
      {
        "endpoint": "MANUSCRIPT",
        "word_template": "APA-7.docx",
        "csl_style": "apa.csl"
      }
    ]
  }
}
