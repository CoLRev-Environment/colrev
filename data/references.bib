@inproceedings{Bulow1998,
    abstract = {The sensitivities of a simple zero-dimensional model which describes the major nutrient processes and the oxygen concentration in the tidal influenced Elbe estuary were studied. For this purpose the Integrated Modeling and Analysis System (IMAS) was used. By using the adjoint model, the impact of the initial model parameters on their counterparts after a period of ten days were studied. By doing this it was determined, which model compartment is acting on the other ones and which is not doing so. By minimizing the misfit between the observational data and their model counterparts (quadratic cost function), it is possible to estimate the the optimal values of the parameters used in the model. The Hessian matrix of the cost function offers the opportunity to determine the posteriori uncertainty of the model parameters. Analyzing the eigenmodes of the Hessian yields those linear combinations of the model parameters, which are most effectively controlled by data, i.e. for which the posteriori confidence interval is significantly smaller than the prior confidence interval.},
    author = {Bulow, KG and Callies, U and Rhodin, A},
    booktitle = {HYDROINFORMATICS `98, VOLS 1 AND 2},
    editor = {Babovic, V and Larsen, LC},
    hash_id = {4e51f71f48cf239988939c414c9c2e4aa02aa60ff587748061faf68313376378},
