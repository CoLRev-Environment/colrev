@article{Howard2019,
    abstract = {Research on task-technology fit (TTF) theory is in need of refinement that is centered around (1) conceptualizations of TTF, (2) operationalizations of TTF, (3) an oversight of ``misfit,″ and (4) an overemphasis on direct effects. We review TTF to place it in the broader nomological net of related constructs. We differentiate task-technology misfit (TTM) from TTF, and we distinguish two types of TTM, ``Too Little″ and ``Too Much.″ Then, we undergo a four-study process to create two satisfactory scales. Finally, we perform two empirical studies to confirm our three-dimensional conceptualization of TTF and TTM in the larger TTF theory framework.},
    article-number = {103134},
    author = {Matt C. Howard and Julia C. Rose},
    doi = {10.1016/j.im.2018.12.002},
